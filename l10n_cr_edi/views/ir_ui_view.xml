<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_l10n_cr_edi_ir_ui_view_form_inherit" model="ir.ui.view">
        <field name="name">l10n_cr_edi.view.form.inherit</field>
        <field name="model">ir.ui.view</field>
        <field name="inherit_id" ref="base.view_view_form"/>
        <field name="arch" type="xml">
            <xpath expr="//notebook[last()]" position="inside">
                <page string="CR parameters" name="cr_parameters">
                    <group>
                        <field name="l10n_cr_edi_addenda_flag"/>
                    </group>
                </page>
            </xpath>
        </field>
    </record>
</odoo>
