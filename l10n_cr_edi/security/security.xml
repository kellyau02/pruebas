<?xml version="1.0" encoding="utf-8"?>
<odoo noupdate="1">
    <record id="country_state_cr_user" model="ir.rule">
        <field name="name">CR States user</field>
        <field name="model_id" ref="base.model_res_country_state"/>
        <field name="groups" eval="[(4, ref('base.group_user'))]"/>
        <field name="domain_force">[('country_id.code','!=','cr')]</field>
        <field name="perm_read" eval="False"/>
        <field name="perm_create" eval="True"/>
        <field name="perm_unlink" eval="True"/>
        <field name="perm_write" eval="True"/>
    </record>
    <record id="country_state_cr_admin" model="ir.rule">
        <field name="name">CR States admin</field>
        <field name="model_id" ref="base.model_res_country_state"/>
        <field name="groups" eval="[(4, ref('base.group_system'))]"/>
        <field name="domain_force">[(1,'=',1)]</field>
        <field name="perm_read" eval="True"/>
        <field name="perm_create" eval="True"/>
        <field name="perm_unlink" eval="True"/>
        <field name="perm_write" eval="True"/>
    </record>
</odoo>

